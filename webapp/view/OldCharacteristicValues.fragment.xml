<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" header="false">	
	<TableSelectDialog id="idCharOldSelect" title="" search="handleCharSearch" liveChange="handleCharSearch" rememberSelections="true"
		confirm="handleOldCharSelection" cancel="handleClose"
		items="{/setOldCharacteristics}" multiSelect="true" contentWidth="300px">
			<columns>
			<Column width="60%" hAlign="Left">
				<header>
					<Text text="Characteristic Values" />
				</header>
			</Column>
			<Column width="40%" hAlign="Left">
				<header>
					<Text text="" />
				</header>
			</Column>
			</columns>
			<ColumnListItem vAlign="Middle">
			<cells>
			<ObjectIdentifier title="{CHARVAL_DESC}" text="{CHAR_VALUE}" ></ObjectIdentifier>
			<ObjectIdentifier  text="{CHARVAL_NUM}" ></ObjectIdentifier>
			</cells>
			</ColumnListItem>
		</TableSelectDialog>
</core:FragmentDefinition>